{
  "name": "homeostat",
  "version": "0.1.0",
  "description": "An agentic fix pipeline that restores your repo to a steady, healthy state",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:property": "vitest run tests/property",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:critical": "vitest run --coverage --include='shared/privacy/**,homeostat/config/**,homeostat/routing/**,homeostat/execution/**'",
    "test:security": "vitest run tests/security",
    "bench": "tsx scripts/run-benchmark.ts",
    "cost:check": "tsx scripts/cost-estimator.ts",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "ai",
    "automation",
    "bug-fixing",
    "github-actions",
    "deepseek",
    "gpt-5",
    "error-handling"
  ],
  "author": "Little Bear Apps",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.0.0",
    "fast-check": "^3.14.0",
    "lockfile-lint": "^4.12.0",
    "prettier": "^3.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "openai": "^4.0.0"
  }
}
